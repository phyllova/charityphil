    /* Close video lightbox */
    // function closeLB() {
    //     if (jQuery('.takeover').find('iframe').length) {
    //         var iframeDiv = jQuery('.takeover').find('iframe');
    //         var iframeSrc = iframeDiv.attr('src');
    //         var iframeNewSrc = iframeSrc.replace(/\&autoplay=1/, '');
    //         iframeDiv.attr('src','');
    //         iframeDiv.attr('src',iframeNewSrc);
    //     }
    //     jQuery('.takeover .lity-close').closest('.lity').removeClass('lity-opened');
    //     jQuery('.takenover .info__content--writeup-hiddencontent').append(jQuery('.takeover .lity-close').closest('.lity'));
    //     jQuery('.info__content').removeClass('takenover');
    // }
    // jQuery('.lity-close').on('click', function() {
    //     closeLB();
    // });
    // jQuery(document).on('keydown', function(event) {
    //     if ((event.key == "Escape") && (jQuery('.takeover').find('iframe').length)) {
    //         closeLB();
    //     }
    // });

    /* Transcript: */
    jQuery('.videotranscript__transcript--switch-state').on('click', function(e) {
    e.preventDefault();
    if (jQuery(this).hasClass('closed')) {
        jQuery(this).removeClass('closed');
        jQuery(this).find('span').html('Hide');
        jQuery('#videotranscript__transcript--box').slideDown();
        jQuery('#videotranscript__transcript--box').focus();
    } else {
        jQuery(this).addClass('closed');
        jQuery(this).find('span').html('View');
        jQuery('#videotranscript__transcript--box').slideUp();
    }
    });

    /* Carousel */
    if ((window.innerWidth < 767) && (!(jQuery('.tot2023__timeline__years.slick-initialized').length))) {
        jQuery('.tot2023__timeline__years').slick({
            dots: true,
            focusOnSelect: true,
            mobileFirst: true,
            accessibility: true
        });
    } else if ((window.innerWidth > 767) && (!(jQuery('.tot2023__timeline__years--nav.slick-initialized').length))) {
        jQuery('.tot2023__timeline__years').slick({
            dots: true,
            focusOnSelect: true,
            mobileFirst: true,
            asNavFor: '.tot2023__timeline__years--nav',
            centerMode: true,
            accessibility: true
        });
        jQuery('.tot2023__timeline__years--nav').slick({
            dots: false,
            focusOnSelect: true,
            mobileFirst: true,
            asNavFor: '.tot2023__timeline__years',
            centerMode: true,
            accessibility: true
        });
    }
    
    jQuery( window ).on( "resize", function() {
        if ((window.innerWidth < 767) && (jQuery('.tot2023__timeline__years--nav.slick-initialized').length)) {
            jQuery('.tot2023__timeline__years').slick('unslick');
            jQuery('.tot2023__timeline__years--nav').slick('unslick');
            jQuery('.tot2023__timeline__years').slick({
                dots: true,
                focusOnSelect: true,
                mobileFirst: true,
                accessibility: true
            });
        } else if ((window.innerWidth > 767) && (!(jQuery('.tot2023__timeline__years--nav.slick-initialized').length))) {
            jQuery('.tot2023__timeline__years').slick('unslick');
            jQuery('.tot2023__timeline__years').slick({
                dots: true,
                focusOnSelect: true,
                mobileFirst: true,
                asNavFor: '.tot2023__timeline__years--nav',
                centerMode: true,
                accessibility: true
            });
            jQuery('.tot2023__timeline__years--nav').slick({
                dots: false,
                focusOnSelect: true,
                mobileFirst: true,
                asNavFor: '.tot2023__timeline__years',
                centerMode: true,
                accessibility: true
            });
        }
    });

});
