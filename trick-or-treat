<script type="db0bcf1629a904001df0011d-text/javascript">
 jQuery(function(){

  /* Video: */
  jQuery('.content-image').on('click', function(e) {
    e.preventDefault();
    jQuery(this).closest('.info__content').addClass('takenover');
    jQuery(this).closest('.info__content').find('.lity').addClass('lity-opened');
    jQuery('.takeover').append(jQuery(this).closest('.info__content').find('.lity'));
    if (jQuery('.takeover').find('iframe').length) {
      var iframeDiv = jQuery('.takeover').find('iframe');
      var iframeSrc = iframeDiv.attr('src');
      iframeSrc += '&autoplay=1';
      iframeDiv.attr('src',iframeSrc);
    }
  });

  jQuery('.lity-close').on('click', function() {
    if (jQuery('.takeover').find('iframe').length) {
      var iframeDiv = jQuery('.takeover').find('iframe');
      var iframeSrc = iframeDiv.attr('src');
      var iframeNewSrc = iframeSrc.replace(/\&autoplay=1/, '');
      iframeDiv.attr('src','');
      iframeDiv.attr('src',iframeNewSrc);
    }
    jQuery(this).closest('.lity').removeClass('lity-opened');
    jQuery('.takenover .info__content--writeup-hiddencontent').append(jQuery(this).closest('.lity'));
    jQuery('.info__content').removeClass('takenover');
  });

    /* Submit form */
    var thankyoupage = '?confirm=true';
    if (window.location.href.indexOf(thankyoupage) > -1) {
        jQuery('.tot-overlay-bg, .tot-overlay-container').removeClass('hidden');
    }

    if (jQuery('form input.error').length) {
        jQuery(this).focus();
        jQuery('#tot2022_bottomsignup_module form input.error').closest('.form-item').addClass('error-form-item');
        jQuery([document.documentElement, document.body]).animate({
            scrollTop: jQuery('#tot2022_bottomsignup_module form').offset().top
        }, 1000);
    }
    
    jQuery('#edit-submitted-email-address, input[name="submitted[email_address]"]').attr('placeholder','Email Address');

    jQuery('.lity-close').click(function() {
        jQuery('.tot-overlay-bg, .tot-overlay-container').addClass('hidden');
    });

	// Slick carousel:
	jQuery('.totevergreen_history_slideshow--carousel').slick({
		// arrows: false,
		dots: true
	});

 });
