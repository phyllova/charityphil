<script type="8ff106802c7a877e44476a24-text/javascript">
jQuery(function(){
    /* Recognition Lightbox */
    if (window.location.href.indexOf("?confirm=true") > -1) {
        var openModal = jQuery('.partner__joinussignup-message').find('.partner__joinussignup__form-entrance__modal');
        jQuery('.partner__joinussignup-message').addClass('current-modal');
        jQuery('body').prepend('<div id="partner__joinussignup-message-lightbox" class="overlay"><' + '/div' + '>');
        jQuery('#partner__joinussignup-message-lightbox').append(openModal);
        jQuery('.partner__joinussignup__form-entrance__button, .partner__joinussignup__form-entrance__close').on('click', function() {
          var closingModal = jQuery('#partner__joinussignup-message-lightbox').children('.partner__joinussignup__form-entrance__modal');
          jQuery('.partner__joinussignup-message.current-modal').find('.partner__joinussignup-message-lightbox').append(closingModal);
          jQuery('.partner__joinussignup-message.current-modal').removeClass('current-modal');
          jQuery('#partner__joinussignup-message-lightbox').remove();
        });
    }

    /* Form validation */
    function ValidateEmail(mailVal) {
        if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mailVal)) {
            jQuery('.partner__joinus__form--container form').submit();
            jQuery('.partner__joinus__form--error').html('Thank you! Form submitting').addClass('active');
            return (true)
        }
        jQuery('.partner__joinus__form--error').html('We are sorry, you did not enter a valid email. Please retry above.').addClass('active');
        jQuery('#partner__joinus .form-item').addClass('error');
        jQuery('#partner__joinus .form-item').focus();
        return (false)
    }
    jQuery('#partner__joinus input.email').attr('placeholder','Email Address');
    jQuery('#edit-submitted-title').attr('placeholder','Company Title (optional)');
    jQuery('#edit-submitted-first-name').attr('placeholder','First Name');
    jQuery('#edit-submitted-last-name').attr('placeholder','Last Name');
    jQuery('#edit-submitted-message').attr('placeholder','Message (optional)');
    jQuery('#edit-submitted-state-location').attr('placeholder','State where you reside');
    jQuery('#partner__joinus #edit-actions input.form-submit').on('click', function(e) {
        e.preventDefault();
        var emailVal = jQuery('#partner__joinus input.email').val();
        ValidateEmail(emailVal);
    });

    if (jQuery('.form-text.error').length) {
        jQuery('.partner__joinus__form--error').html('Please address the form errors above and resubmit.');
        jQuery('.partner__joinus__form--error').show();
        var distancefromTop = window.innerHeight + jQuery('.partner__joinus__form--error').offset().top;
        jQuery([document.documentElement, document.body]).animate({
            scrollTop: distancefromTop
        }, 2000);
    }

});
</script>
