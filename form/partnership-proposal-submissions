<script src="https://code.jquery.com/jquery-3.5.1.min.js" type="8ff106802c7a877e44476a24-text/javascript"></script>
<script type="8ff106802c7a877e44476a24-text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
<script type="8ff106802c7a877e44476a24-text/javascript">
jQuery(function(){

    /* Carousel */
    if (window.innerWidth < 768) {
        jQuery('.partner__transformational--modules').slick({
            arrows: true,
            dots: false,
            focusOnChange: true,
            mobileFirst: true,
            accessible: true
        });
    }
    
    jQuery( window ).on( "resize", function() {
        if (window.innerWidth > 767) {
            jQuery('.partner__transformational--modules').slick('unslick');
        } else if (!(jQuery('.partner__transformational--modules.slick-initialized').length)) {
            jQuery('.partner__transformational--modules').slick({
                arrows: true,
                dots: false,
                focusOnChange: true,
                mobileFirst: true,
                accessible: true
            });
        }
    });

});
</script>
