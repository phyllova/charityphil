        body #header #block-menu-block-2 ul.menu li.first .menu__link,
        body #header #block-menu-block-2 ul.menu li.index-1 .menu__link {
                border-radius: 0;
        }
        body #header #block-menu-block-2 {
                right: 0;
        }
        #header #block-webform-client-block-29586 .webform-component-email input {
                width: 14em;
        }
}

