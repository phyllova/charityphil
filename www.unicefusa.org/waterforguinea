<div class="well-section section-1" id="section-1">
<div class="bubbles pipe">
<div class="bubbles__container background1"><div class="bubbles__bubble"></div></div>
<div class="bubbles__container background2"><div class="bubbles__bubble"></div></div>
<div class="bubbles__container background3"><div class="bubbles__bubble"></div></div>
<div class="bubbles__container background4"><div class="bubbles__bubble"></div></div>
<div class="bubbles__container background5"><div class="bubbles__bubble"></div></div>
<img src="https://www.unicefusa.org/sites/default/files/water-for-guinea-images/wfg-images/bubblebackgroundhover.png" class="hidden" />
