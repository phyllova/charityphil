<script src="https://code.jquery.com/jquery-3.4.1.min.js" type="a053b525fdcc92eadd630012-text/javascript"></script>
<script type="a053b525fdcc92eadd630012-text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/3.1.0/jquery-migrate.min.js"></script>

<script type="a053b525fdcc92eadd630012-text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.js"></script>

<script type="a053b525fdcc92eadd630012-text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.7.2/jquery.flexslider-min.js"></script>

<script src="https://cdn.jsdelivr.net/npm/foundation-sites@6.5.3/dist/js/foundation.min.js" type="a053b525fdcc92eadd630012-text/javascript"></script>
<script type="a053b525fdcc92eadd630012-text/javascript">

  $(document).foundation();

  $(function() {

    // left: 37, up: 38, right: 39, down: 40,
    // spacebar: 32, pageup: 33, pagedown: 34, end: 35, home: 36
    var keys = {37: 1, 38: 1, 39: 1, 40: 1};

    function preventDefault(e) {
      e = e || window.event;
      if (e.preventDefault)
          e.preventDefault();
      e.returnValue = false;  
    }

    function preventDefaultForScrollKeys(e) {
        if (keys[e.keyCode]) {
            preventDefault(e);
            return false;
        }
    }

    function disableScroll() {
      if (window.addEventListener) // older FF
          window.addEventListener('DOMMouseScroll', preventDefault, false);
      document.addEventListener('wheel', preventDefault, {passive: false}); // Disable scrolling in Chrome
      window.onwheel = preventDefault; // modern standard
      window.onmousewheel = document.onmousewheel = preventDefault; // older browsers, IE
      window.ontouchmove  = preventDefault; // mobile
      document.onkeydown  = preventDefaultForScrollKeys;
    }

    function enableScroll() {
        if (window.removeEventListener)
            window.removeEventListener('DOMMouseScroll', preventDefault, false);
        document.removeEventListener('wheel', preventDefault, {passive: false}); // Enable scrolling in Chrome
        window.onmousewheel = document.onmousewheel = null; 
        window.onwheel = null; 
        window.ontouchmove = null;  
        document.onkeydown = null;  
    }

    jQuery('.intro__writeup--cta').on('click', function() {
      $([document.documentElement, document.body]).animate({
        scrollTop: $('#module-3').position().top - 62
      }, 1000);
    });


  function isDesktop() {
  
    jQuery('.panel-pane.pane-entity-field.pane-node-body').css({'opacity':'1'});

    // start at bottom and show the content when there
    window.scrollTo(0,$('#content').height());

    $('.jumpnav__container').height(document.body.scrollHeight - (document.getElementById('section-4-module-1').scrollHeight + jQuery('#section-4-module-1').position().top) - 91 );

    var meterHeight = $('#content').height() - window.innerHeight;
    var heightPercent = 0;

    var section1Height = document.getElementById('section-1').scrollHeight;
    var lastScrollTop = 0;
    var underSurface = true;
    var instructionsTop = $('#instructions').offset().top;
    heightPercent = 90 * (1 - ((((window.innerHeight / 2) + window.scrollY) - (document.getElementById('section-4-module-1').scrollHeight + jQuery('#section-4-module-1').position().top)) / ((document.body.scrollHeight) - (document.getElementById('section-4-module-1').scrollHeight + jQuery('#section-4-module-1').position().top))));
    heightString = (90 - parseInt(heightPercent)).toString();
    $('.jumpnav__meter--feet').html(heightString);

    $(document).on('scroll', function() {

      if (window.scrollY < document.getElementById('section-3').scrollHeight) {
        underSurface = false;
        $('#instructions').removeClass('follow');
        setTimeout(function() {
          $('#instructions').removeClass('outofsight');
        }, 1000);
      }

      if (underSurface) {
        if (window.scrollY < ((document.body.scrollHeight - window.innerHeight) - 140)) {
          $('#instructions').addClass('outofsight');
        } else if (window.scrollY > ((document.body.scrollHeight - window.innerHeight) - 140)) {
          $('#instructions').fadeOut();
          setTimeout(function() {
            $('#instructions').removeClass('outofsight');
            $('#instructions').fadeIn();
          }, 1000);
        }
      }

      if ((window.scrollY < (instructionsTop - window.innerHeight)) && (!$('#instructions.follow').length)) {
        $('#instructions').addClass('follow');
      } else if ((window.scrollY > (instructionsTop - window.innerHeight)) && ($('#instructions.follow').length)) {
        $('#instructions').removeClass('follow');
      }

      var st = $(this).scrollTop();
      if ((st > lastScrollTop) && (!$('#instructions.slowin').length)) {
        $('#instructions').addClass('slowin');
      } else if ((st < lastScrollTop) && ($('#instructions.slowin').length)) {
        $('#instructions').removeClass('slowin');
      }
      lastScrollTop = st;

      if (window.scrollY < (document.body.scrollHeight - window.innerHeight)) {
        $('#header').addClass('scrolled');
      } else {
        $('#header').removeClass('scrolled');
      }



      $('.yardstick .inch').each(function() {
        if (((window.scrollY - document.getElementById('section-4').scrollHeight) / ((document.body.scrollHeight  - document.getElementById('section-4').scrollHeight) - window.innerHeight)) <= (($(this).position().top) / ($('.yardstick').height() - ($('.yardstick').height() / 10)))) {
          $(this).addClass('visible');
        } else if ((window.scrollY / (document.body.scrollHeight - window.innerHeight)) > (($(this).position().top) / ($('.yardstick').height() - ($('.yardstick').height() / 10)))) {
          $(this).removeClass('visible');
        }
      });

      heightPercent = 90 * (1 - ((((window.innerHeight / 2) + window.scrollY) - (document.getElementById('section-4-module-1').scrollHeight + jQuery('#section-4-module-1').position().top)) / ((document.body.scrollHeight) - (document.getElementById('section-4-module-1').scrollHeight + jQuery('#section-4-module-1').position().top))));
      heightString = (90 - parseInt(heightPercent)).toString();
      if (90 - parseInt(heightPercent) === 0) {
        $('.jumpnav__meter').fadeOut();
      } else if (90 - parseInt(heightPercent) < 0) {
        $('.jumpnav__meter').hide();
      } else if (!$('.jumpnav__meter').is(':visible')) {
        $('.jumpnav__meter').fadeIn();
      }
      $('.jumpnav__meter--feet').html(heightString);
    });

    $(document).keyup(function(e) {
      if (e.keyCode === 27) $('.takeover .lity-close').click();
    });
  }

  var isAlreadyDesktop = false;
  if (((window.innerWidth > 1023) && ($('.jumpnav__container').length)) && (isAlreadyDesktop === false)) {
    isAlreadyDesktop = true;
    isDesktop();
  }
  window.addEventListener('resize', function() {
    if (((window.innerWidth > 1023) && ($('.jumpnav__container').length)) && (isAlreadyDesktop === false)) {
      isAlreadyDesktop = true;
      isDesktop();
    }
  });


  var bubblesClicked = 0;
  $('.bubbles__info-container .bubbles__bubble').on('click', function() {
    if (!($(this).hasClass('bubbles__bubble--viewed'))) {
      $(this).addClass('bubbles__bubble--viewed');
      bubblesClicked++;
    }
  });

  $('.content-image').on('click', function(e) {
    e.preventDefault();
    if ($('.jumpnav__container').length) {
      $(this).closest('.info__content').addClass('takenover');
      $(this).closest('.info__content').find('.lity').addClass('lity-opened');
      $('.takeover').append($(this).closest('.info__content').find('.lity'));
    } else {
      $(this).closest('.blog__entry--media').addClass('takenover');
      $(this).closest('.blog__entry--media').find('.lity').addClass('lity-opened');
      $('.takeover').append($(this).closest('.blog__entry--media').find('.lity'));
    }
    if ($('.takeover').find('iframe').length) {
      var iframeDiv = $('.takeover').find('iframe');
      var iframeSrc = iframeDiv.attr('src');
      iframeSrc += '?autoplay=1';
      iframeDiv.attr('src',iframeSrc);
    }
    disableScroll();
  });

  $('.lity-close').on('click', function() {
    if (($('.jumpnav__container').length) && (window.innerWidth > 1023)) {
      if ($('.takenover').hasClass('bubbles__info-container')) {
        if (bubblesClicked === 7) {
          $([document.documentElement, document.body]).animate({
            scrollTop: $('#section-1').position().top - $('#bubbles__theater').position().top - $('#section-3').height() - $('#section-3-module-2').height()
          }, 1000);
        }
      } else {
        $([document.documentElement, document.body]).animate({
          scrollTop: $('#section-1').position().top - $('#bubbles__theater').position().top - $('#section-3').height()
        }, 1000);
      }
    }
    if ($('.takeover').find('iframe').length) {
      var iframeDiv = $('.takeover').find('iframe');
      var iframeSrc = iframeDiv.attr('src');
      var iframeNewSrc = iframeSrc.replace(/\?autoplay=1/, '');
      iframeDiv.attr('src','');
      iframeDiv.attr('src',iframeNewSrc);
    }
    $(this).closest('.lity').removeClass('lity-opened');
    $('.takenover .info__content--writeup-hiddencontent').append($(this).closest('.lity'));
    $('.info__content').removeClass('takenover');
    enableScroll();

  });

  function isScrolled() {
    if (window.scrollY > 1) {
      $('#header').addClass('scrolled');
    } else {
      $('#header').removeClass('scrolled');
    }
  }
  if (!($('.jumpnav__container').length)) {
    isScrolled();
    $(document).on('scroll', function() {
      isScrolled();
    });
  }

  $('.more').on('click', function() {
    $(this).fadeOut();
    $(this).closest('.blog__entry--info-slug').find('.more__content').slideDown();
  });
});

