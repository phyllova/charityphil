<script type="24e099046d111f8c7807cd24-text/javascript">
var currentCity = document.getElementById('breadcrumb__city').innerHTML;
document.getElementById('navbar__breadcrumbs--city').innerHTML = currentCity;

 jQuery(function(){

    var lastScrollTop = 0, delta = 5;
    jQuery(window).scroll(function(){
        var nowScrollTop = jQuery(this).scrollTop();
        if (Math.abs(lastScrollTop - nowScrollTop) >= delta) {
            if (nowScrollTop > lastScrollTop){
                jQuery('.navbar-menu, #content').addClass('scrunch');
            } else {
                jQuery('.navbar-menu, #content').removeClass('scrunch');
            }
            lastScrollTop = nowScrollTop;
        }
        if (jQuery(window).scrollTop() < 5) {
            jQuery('.navbar-menu').removeClass('scrunch');
        }
    });


    if (jQuery('.navbar-message.achievement').html()) {
        jQuery('.navbar-notice.achievement').html(jQuery('.navbar-message.achievement').html());
        jQuery('#content').addClass('hasnavmessage');

    }
    if (jQuery('.navbar-message.emergency').html()) {
        jQuery('.navbar-notice.emergency').html(jQuery('.navbar-message.emergency').html());
        jQuery('#content').addClass('hasnavmessage');
    }

    // jQuery('.navbar__breadcrumbs--city').html(jQuery('#breadcrumb__city').html());

 });
